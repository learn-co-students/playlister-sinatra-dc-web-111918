<h1>Show Artists</h1>
<!DOCTYPE html>
<html lang="en" dir="ltr">
  <head>
    <meta charset="utf-8">
    <title><%=@artist.name%></title>
  </head>
  <body>
    <h2>Songs</h2>
    <ul>
    <% @artist.songs.each do |song|%>
        <li><a href="/songs/<%= song.slug %>"><%=song.name%></a> -
          <% song.genres.each do |genre|%>
        <a href="/genres/<%= genre.slug %>"><%= genre.name %> </a></li>
        <%end%>
    <%end%>
    </ul>
  </body>
</html>
